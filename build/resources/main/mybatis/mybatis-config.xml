        <typeAlias type="com.joeunseo.hotel.model.domain.BookingModel" alias="bookingModel"></typeAlias>
        <typeAlias type="com.joeunseo.hotel.model.domain.HotelModel" alias="hotelModel"></typeAlias>
        <typeAlias type="com.joeunseo.hotel.model.domain.ReplyModel" alias="replyModel"></typeAlias>
    </typeAliases>
